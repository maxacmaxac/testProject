[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group Abaloapp
	owner = group user/maxacmaxac
[access "refs/for/refs/*"]
	read = group Abaloapp
	read = group user/maxacmaxac
	push = group Abaloapp
	push = group user/maxacmaxac
	label-Code-Review = -2..+2 group Abaloapp
	label-Code-Review = -2..+2 group user/maxacmaxac
	label-Verified = -1..+1 group Abaloapp
	label-Verified = -1..+1 group user/maxacmaxac
	rebase = group Abaloapp
	rebase = group user/maxacmaxac
	removeReviewer = group Abaloapp
	removeReviewer = group user/maxacmaxac
	submit = group Abaloapp
	submit = group user/maxacmaxac
[access "refs/heads/*"]
	read = group Abaloapp
	read = group user/maxacmaxac
	create = group Abaloapp
	create = group user/maxacmaxac
	push = group Abaloapp
	push = +force group user/maxacmaxac
	pushMerge = group Abaloapp
	pushMerge = group user/maxacmaxac
[access "refs/tags/*"]
	push = +force group user/maxacmaxac
	pushTag = group user/maxacmaxac
	pushSignedTag = group user/maxacmaxac
